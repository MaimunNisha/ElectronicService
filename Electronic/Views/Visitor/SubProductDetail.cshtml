@model Electronic.Models.SubProductCategoryListModel

@{
    ViewData["Title"] = "SubProductDetail";
    Layout = "~/Views/Shared/_Visitor_Layout.cshtml";
}
<div id="tt-pageContent">
    <div class="tt-breadcrumb" style="background-image: url('@Url.Content("~/wp-content/uploads/2020/08/breadcrumb_bg.jpg")')">
        <div class="container container-lg-fluid">

        <div class="breadcrumb-area">
                <p id="breadcrumbs"><span><span><a href="@Url.Action("Index", "Home")">Home</a></span> / <span class="breadcrumb_last" aria-current="page">Our Product Details</span></span></p>
            </div>
        </div>
    </div>
    <div class="block">
        <div class="container">
            <article id="post-1522" class="post-1522 page type-page status-publish hentry">

                <div class="entry-content">
                    <div data-elementor-type="wp-page" data-elementor-id="1522" class="elementor elementor-1522">

            <section class="elementor-section elementor-top-section elementor-element elementor-section-stretched elementor-section-full_width" data-element_type="section">
                <div class="elementor-container elementor-column-gap-no">
                    <div class="elementor-column elementor-col-100 elementor-top-column">
                        <div class="elementor-widget-wrap elementor-element-populated">
                            <div class="elementor-widget-container">
                                <div class="section-indent">
                                    <div class="container container-lg-fluid">
                                        <div class="layout01 layout01__revers layout01__single-img">
                                            
                                            <div class="layout01__img">
                                                <div class="tt-img-main">
                                                    <img loading="lazy" decoding="async" width="405" height="405" src="~/@Model.Sub_P_C_Image" alt="@Model.Sub_P_C_Name" />
                                                </div>
                                            </div>
                                            <div class="layout01__content">
                                                <div class="layout01__content-wrapper">
                                                    <div class="section-title text-left">
                                                        <div class="section-title__01">Why @Model.Sub_P_C_Name?</div>
                                                        <div class="section-title__02">Details:</div>
                                                    </div>
                                                    <p>@Model.Sub_P_C_Description</p>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
        
        
    </div>
    </div>
    </article>
    </div>
    </div>
</div>
