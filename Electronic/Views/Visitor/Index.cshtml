			<div class="elementor-container elementor-column-gap-no">
				<div class="elementor-column elementor-col-100 elementor-top-column elementor-element elementor-element-23f47d8"
					 data-id="23f47d8" data-element_type="column">
					<div class="elementor-widget-wrap elementor-element-populated">
						<div class="elementor-element elementor-element-13dd638 elementor-widget elementor-widget-electrician_project"
							 data-id="13dd638" data-element_type="widget"
							 data-widget_type="electrician_project.default">
							<div class="elementor-widget-container">
								<div class="section-indent">
									<div class="container container-md-fluid">
										<div class="section-title max-width-01">
											<div class="section-title__01">#electricians</div>
											<div class="section-title__02">Our Products</div>
											<div class="section-title__03">
											</div>
										</div>
										<div id="filter-nav">
									
											<ul id="filter-nav">
												<li class="active">
													<a class="gallery-navitem" href="filter-all">All Products</a>
												</li>
												@foreach (var category in Model.categoryList.Select(c => c.C_Name).Distinct())
												{
													<li>
														<a class="gallery-navitem" href="@category.Replace(" ", "_").ToLower()">@category</a>
													</li>
												}
											</ul>



										</div>
										<div id="filter-layout"
											 class="row justify-content-center tt-gallery-wrapper js-wrapper-gallery">
											@foreach (var product in Model.categoryList)
											{
												var uniqueClass = product.C_Name.Replace(" ", "_").ToLower() + "_" + product.C_Id;

												<div class="col-6 col-md-3 show filter-all @uniqueClass">
													<a href="@product.C_Image" class="tt-gallery">
														<div class="gallery__icon"></div>
														<img loading="lazy" decoding="async" width="222" height="222"
															 src="@product.C_Image" class="attachment-full size-full" alt="@product.C_Name"
															 style="max-height: 300px; margin: 0 auto 1rem;" />
														<h4 class="tt-box02 tt-box02__title" style="color: #000000;">@product.C_Name</h4>
													</a>
												</div>
											}



										</div>
										<script>
								              	document.addEventListener("DOMContentLoaded", function () {
												const filterNavItems = document.querySelectorAll(".gallery-navitem");
												const galleryItems = document.querySelectorAll("#filter-layout .col-6");

												filterNavItems.forEach((item) => {
													item.addEventListener("click", function (e) {
														e.preventDefault();

														// Update active class in navigation
														document.querySelector("#filter-nav .active").classList.remove("active");
														this.parentElement.classList.add("active");

														// Get the category name from href and format it correctly
														let filterClass = this.getAttribute("href")
															.replace("filter-", "")
															.replace("-", "_")
															.toLowerCase();

														// Show/hide products based on selected category
														galleryItems.forEach((galleryItem) => {
															let itemClasses = galleryItem.className.split(" ");

															// If "all" is selected, show everything
															if (filterClass === "all") {
																galleryItem.style.display = "block";
															}
															// Otherwise, show only matching categories
															else {
																let matchFound = itemClasses.some(cls => cls.startsWith(filterClass));
																galleryItem.style.display = matchFound ? "block" : "none";
															}
														});
													});
												});
											});


										</script>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
